(selector){
  # minimum mass cut on electron for num. stability
  Mass  11 -11  0.1  E_CMS
  # minimum lepton pt and eta
##  "PT" 991 5.0,E_CMS:5.0,E_CMS [PT_UP]
##  "Eta" 991 -2.7,2.7:-2.7,2.7 [PT_UP]
  "PT" 90 5.0,E_CMS:5.0,E_CMS [PT_UP]
  "Eta" 90 -2.7,2.7:-2.7,2.7 [PT_UP]
}(selector)

(integration){
FINISH_OPTIMIZATION = Off
}(integration)

(isr){
